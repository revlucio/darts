{
  "name": "darts",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/revlucio/darts",
  "author": "Luke Elliott <revlucio@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build:web": "parcel build src/web/index.html --out-dir dist/web",
    "build:api": "babel src/api/index.js -d dist",
    "build": "rm -rf dist && yarn build:web && yarn build:api",
    "start:api": "node src/api/index.js",
    "start:web": "parcel src/web/index.html",
    "start": "node dist/index.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "express": "^4.17.1",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "sequelize": "^5.18.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@types/express": "^4.17.1",
    "@types/sequelize": "^4.28.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "husky": "^3.0.4",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1"
  }
}
